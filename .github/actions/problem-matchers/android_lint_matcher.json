{
  "problemMatcher": [
    {
      "owner": "android-lint-file",
      "pattern": [
        {
          "regexp": "^([^:\\n]+):\\s+(Warning|Error):\\s+(.+)\\s+\\[(.+)\\]$",
          "file": 1,
          "severity": 2,
          "message": 3,
          "code": 4,
          "kind": "file"
        }
      ]
    },
    {
      "owner": "android-lint-line",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):\\s+(Warning|Error):\\s+(.+)\\s+\\[(.+)\\]$",
          "file": 1,
          "line": 2,
          "severity": 3,
          "message": 4,
          "code": 5
        }
      ]
    }
  ]
}
